{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es6"],
    "target": "ES5",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/index.d.ts", "test/**/*.spec.js"],
}
